{
  "name": "leagueoflegends",
  "description": "League of Legends CLI.",
  "license": "MIT",
  "author": "Luke Al-Saba",
  "url": "https://al-saba.com/",
  "homepage": "https://www.npmjs.com/package/leagueoflegends",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukealsaba/leagueoflegends.git"
  },
  "bugs": {
    "url": "https://github.com/lukealsaba/leagueoflegends/issues"
  },
  "version": "3.1.0",
  "main": "lib/cli.js",
  "bin": {
    "leagueoflegends": "lib/cli.js"
  },
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.js -p",
    "clean": "rimraf lib packed",
    "dev": "webpack --config webpack.config.js -d -w",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "pack": "pkg . --out-path packed",
    "prepublishOnly": "npm run build && echo '#!/usr/bin/env node' | cat - lib/cli.js > temp && mv temp lib/cli.js",
    "test": "npm run lint:fix && npm run testonly:cov",
    "testonly": "NODE_ENV=test jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit --no-cache",
    "testonly:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/register": "^7.4.4",
    "async-to-gen": "^1.4.0",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "node-fetch": "^2.6.0",
    "ora": "^3.4.0",
    "path": "^0.12.7",
    "wide-align": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.1",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/runtime": "^7.5.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-register": "^6.26.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.0",
    "moment-timezone": "^0.5.26",
    "pkg": "^4.3.7",
    "prettier": "^1.18.2",
    "prettier-package-json": "^2.0.1",
    "rimraf": "^2.6.3",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2"
  },
  "keywords": [
    "leagueoflegends",
    "cli",
    "esports",
    "riot",
    "games",
    "lol",
    "lcs",
    "lec",
    "lck",
    "lpl",
    "lcs-academy"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "testPathIgnorePatterns": [
      "node_modules/"
    ]
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pkg": {
    "scripts": [
      "lib/*.js"
    ],
    "targets": [
      "node8-macos",
      "node8-linux",
      "node8-win"
    ]
  },
  "directories": {
    "lib": "lib"
  }
}
